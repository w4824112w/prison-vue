<div class="row">
  <div class="container">
  <h3>共 <%= @pages %> 页</h3>
    <table class="table">
    <tr>
        <th>Phone</th>
        <th>Errors</th>
        <th>Device Name</th>
        <th>Device Type</th>
        <th>System Version</th>
        <th>APP Version</th>
        <th>Created At</th>
    </tr>
    <% @logs.each do |log| %>
        <tr>
        <td><%= log.phone %></td>
        <td><%= log.contents %></td>
        <td><%= log.device_name %></td>
        <td><%= log.device_type %></td>
        <td><%= log.sys_version %></td>
        <td><%= log.app_version %></td>
        <td><%= log.created_at %></td>
        </tr>
    <% end %>
    </table>
  </div>
</div>